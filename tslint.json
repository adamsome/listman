{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "max-line-length": { "options": 120 },
    "member-access": [true, "no-public"],
    "object-literal-sort-keys": false,
    "quotemark": false,
    "semicolon": [true, "never"],
    "trailing-comma": false
  }
}
